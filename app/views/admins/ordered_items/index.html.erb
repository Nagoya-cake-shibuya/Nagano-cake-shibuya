<div class="container">
	<div class="row">
		<div class="col-md-10 col-md-offset-1">
			<h1>注文一覧画面</h1>
			<div class = "genre-list">
				<table class="table">
					<thead>
						<tr>
							<th>購入日時</th>
							<th>購入者</th>
							<th>注文個数</th>
							<th>注文ステータス</th>
						</tr>
					</thead>
					<tbody>
			 			<% @ordered_items.each do |ordered_item| %>
				 			<tr>
					  			<td><%= ordered_item.created_at %></td>
					  			
							</tr>
						<% end %>
					</tbody>
				</table>
			 </div>
		 </div>
	</div>
</div>

